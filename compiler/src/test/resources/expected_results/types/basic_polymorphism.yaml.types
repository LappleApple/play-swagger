#/definitions/Cat ->
	AllOf(
		TypeReference(#/definitions/Pet)
		TypeDef("#Cat", Seq(
			Field("#huntingSkill", Str))))
#/definitions/CatNDog ->
	AllOf(
		TypeReference(#/definitions/Dog)
		TypeReference(#/definitions/Cat))
#/definitions/Dog ->
	AllOf(
		TypeReference(#/definitions/Pet)
		TypeDef("#Dog", Seq(
			Field("#packSize", Intgr))))
#/definitions/Labrador ->
	AllOf(
		TypeReference(#/definitions/Dog)
		TypeDef("#Labrador", Seq(
			Field("#cuteness", Intgr))))
#/definitions/Pet ->
	TypeDef("#Pet", Seq(
		Field("#name", Str), 
		Field("#petType", Str)))